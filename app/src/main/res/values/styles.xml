<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="OutlinedBox" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:background">@color/primary_100</item>
        <item name="boxCornerRadiusBottomEnd">8dp</item>
        <item name="boxCornerRadiusBottomStart">8dp</item>
        <item name="boxCornerRadiusTopEnd">8dp</item>
        <item name="boxCornerRadiusTopStart">8dp</item>
        <item name="hintTextColor">@color/secondary_100</item>
        <item name="boxStrokeColor">@color/secondary_100</item>
        <item name="android:textColorHint">@color/tertiary_100</item>
        <item name="fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="SearchView" parent="Widget.AppCompat.SearchView">
        <item name="android:textColorPrimary">@color/tertiary_100</item>
        <item name="android:backgroundTint">@color/tertiary_100</item>
    </style>

</resources>